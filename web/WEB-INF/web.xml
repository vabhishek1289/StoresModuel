    </servlet>
    <servlet>
        <servlet-name>maintenance</servlet-name>
        <servlet-class>com.store.functions.maintenance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Maintenance_item_subitem</servlet-name>
        <servlet-class>com.store.functions.Maintenance_item_subitem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>m</servlet-name>
        <servlet-class>com.store.functions.m</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login_store</servlet-name>
        <url-pattern>/Login_store</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_item</servlet-name>
        <url-pattern>/Add_item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_sub_item</servlet-name>
        <url-pattern>/Add_sub_item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_item</servlet-name>
