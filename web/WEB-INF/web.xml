    <servlet>
        <servlet-name>Add_sub_item</servlet-name>
        <servlet-class>com.store.functions.Add_sub_item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_item</servlet-name>
        <servlet-class>com.store.functions.Delete_item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_sub_item</servlet-name>
        <servlet-class>com.store.functions.Delete_sub_item</servlet-class>
